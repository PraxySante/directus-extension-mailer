{
  "name": "directus-extension-Mailer",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-endpoint"
  ],
  "directus:extension": {
    "type": "endpoint",
    "path": "index.js",
    "source": "src/index.js",
    "host": "^9.10.0"
  },
  "watch": {
    "build": "{src,test}/*.js"
  },
  "scripts": {
    "build": "directus-extension build",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.10.0"
  },
  "dependencies": {
    "npm-watch": "^0.11.0"
  }
}
